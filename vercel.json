{
  "version": 2,
  "functions": {
    "backend/api/items.go": {
      "runtime": "go"
    }
  },
  "routes": [
    {
      "src": "/api/items",
      "dest": "backend/api/items.go"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ]
}
