{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/items.go",
      "use": "@vercel/go"
    },
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/items",
      "dest": "backend/api/items.go"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
