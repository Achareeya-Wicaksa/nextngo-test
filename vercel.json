{
  "functions": {
    "backend/api/*.go": {
      "runtime": "vercel-go@0.5.0"
    }
  },
  "builds": [
    {
      "src": "src/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "backend/api/*.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/$1"
    }
  ]
}
